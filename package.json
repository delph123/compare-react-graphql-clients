{
  "name": "compare-react-graphql-clients",
  "version": "1.0.0",
  "description": "Demo site to compare GraphQL clients for react (Apollo, React-Query, Redux-Thunk)",
  "private": true,
  "author": {
    "name": "delph123"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/delph123/compare-react-graphql-clients.git"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@reduxjs/toolkit": "^1.8.3",
    "@tanstack/react-query": "^4.0.10",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-graphql": "^0.12.0",
    "graphql": "^16.5.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "front": "vite",
    "back": "nodemon src/server/index.js",
    "start": "npm-run-all --parallel back front",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "coverage": "vitest run --coverage",
    "test": "vitest",
    "test:ui": "vitest --ui"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.2",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@vitejs/plugin-react": "^2.0.1",
    "@vitest/coverage-c8": "^0.22.1",
    "@vitest/ui": "^0.22.1",
    "eslint": "^8.21.0",
    "jsdom": "^20.0.0",
    "nodemon": "^2.0.19",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4",
    "vite": "^3.0.8",
    "vitest": "^0.22.1"
  }
}
